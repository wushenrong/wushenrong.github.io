{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/collections/_posts",
      "title": "Posts"
    },
    {
      "path": "[[workspace]]/collections/_drafts",
      "title": "Drafts"
    }
  ],
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.data.files": [
    {
      "file": "[[workspace]]/_data/socials.yaml",
      "fileType": "yaml",
      "id": "socials",
      "schema": {
        "properties": {
          "platform": {
            "title": "Platform",
            "type": "string"
          },
          "platform_name": {
            "title": "Platform Name",
            "type": "string"
          },
          "user_url": {
            "format": "uri",
            "title": "Profile page",
            "type": "string"
          }
        },
        "required": [
          "platform_name",
          "platform",
          "user_url"
        ],
        "type": "object"
      },
      "title": "Socials"
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.framework.startCommand": "bundle exec jekyll serve --draft --livereload",
  "frontMatter.global.timezone": "America/New_York",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.preview.pathName": "/{{year}}/{{month}}/{{day}}/",
  "frontMatter.preview.trailingSlash": true,
  "frontMatter.taxonomy.contentTypes": [
    {
      "fields": [
        {
          "name": "title",
          "required": true,
          "title": "Title",
          "type": "string"
        },
        {
          "name": "description",
          "required": true,
          "title": "Description",
          "type": "string"
        },
        {
          "name": "slug",
          "title": "Slug",
          "type": "string"
        },
        {
          "default": "{{now}}",
          "isPublishDate": true,
          "name": "date",
          "title": "Publishing date",
          "type": "datetime"
        },
        {
          "default": "{{now}}",
          "isModifiedDate": true,
          "name": "last_modified_at",
          "title": "Last modified date",
          "type": "datetime"
        },
        {
          "default": true,
          "name": "jektex",
          "title": "Math support",
          "type": "boolean"
        }
      ],
      "name": "default",
      "pageBundle": false
    }
  ],
  "frontMatter.taxonomy.slugTemplate": "{{title}}",
  "frontMatter.website.host": "https://wushenrong.github.io"
}
